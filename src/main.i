  if((!(~(oldButtons)&((1<<1))) && (~buttons & ((1<<1))))) {
   checkShot();
   lockFlag = 0;
  }
  if(actualScore > 43) {
   state = WIN;
   return;
  }
  if(health > 4) {
   state = LOSE;
   return;
  }

  *(volatile unsigned short *)0x04000014 = hOff;
  *(volatile unsigned short *)0x04000016 = vOff;
  updateOAM();

  DMANow(3, shadowOAM, ((OBJ_ATTR*)(0x7000000)), 512);
  waitForVblank();
 }
}

