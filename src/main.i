void checkShot(){
 if(80 > enemies.row + 8 && 120 > enemies.col
  && 80 < enemies.row + 32 && 120 < enemies.col + 32
  && enemies.isDead == 7) {
  enemies.aniState = 0;
  enemies.isDead = 15;
  score++;
  actualScore++;
  shotFlag = 0;
  oldScopeBigRow = scopeBigRow;
  oldScopeBigCol = scopeBigCol;
  if(rand()%2 == 0) {
   whichBloodSprite = 24;
  } else {
   whichBloodSprite = 26;
  }

  if (score > 9) {
   score = 0;
   scoreTenth++;
   if(actualScore > 99) {
    state = WIN;
   }
  }
  playSoundB(TeemoDeath, 62208, 48000);
 } else {
  playSoundB(GunShot,11121,22050);
 }
}

void checkDead(){
 if(shotFlag == 0) {
  enemyBloodCounter++;
  if(enemyBloodCounter > 15) {
   shotFlag = 1;
   enemyBloodCounter = 0;
   flyingBloodFlag = 0;
  }
 }
 if(flyingBloodFlag == 0) {
  enemyBloodCounter++;
  if(enemyBloodCounter == 10) {
   bloodVelocity++;
  }
  if(enemyBloodCounter == 20) {
   bloodVelocity++;
  }
  if(enemyBloodCounter > 30) {
   flyingBloodFlag = 1;
   bloodVelocity = 1;
   enemyBloodCounter = 0;
  }
 }

 if(enemies.isDead == 15) {
  if(enemies.deathCounter > 70){
   enemies.aniState = 16;
  }
  enemies.deathCounter++;
 }
}

void respawn(){
 if(enemies.isDead == 15 && enemies.deathCounter > 160) {
  enemies.aniState = 0;
  enemies.deathCounter = 0;
  enemies.isDead = 7;
  currBush = rand()%5;
 }
}

void didHeShoot(){
 if(enemies.isDead == 7 && rand()%enemyFreq == 1 && shootingFlag == 1) {
  shootingFlag = 0;
  health++;
  bloodCounter = 0;
  playSoundB(TeemoShot,18638,44100);
 }

 if(shootingFlag == 0) {
  bloodCounter ++;
  if(bloodCounter > 20) {
   shootingFlag = 1;
  }
 }
}

void didHeMove() {
 if(enemies.isDead == 7 && rand()%enemyFreq == 2 && shootingFlag == 1) {
  currBush = rand()%5;
  playSoundB(TeemoMove, 97844, 44100);
 }
}

void checkLevel() {
 if(actualScore % 5 == 0 && levelFlag < 2 && actualScore > 0) {
  soundCounter++;
  if(levelFlag == 0) {
   level++;
   health = 0;
   enemyFreq = enemyFreq - 100;
   levelFlag = 1;
   levelSpriteFlag = 0;
  }
  if(soundCounter > 51) {
   playSoundB(CaitlynShot, 76224, 44100);
   levelFlag = 2;
   soundCounter = 0;
   levelSpriteFlag = 1;
   levelUpVelocity = 0;
  }
 }
 if(actualScore % 5 == 1) {
  levelFlag = 0;
 }
 if(levelSpriteFlag == 0 && soundCounter % 5 == 0) {
  levelUpVelocity++;
 }
}

void pause()
{
 *(unsigned short *)0x4000000 = 4 | (1<<10);

 loadPalette(SplashPal);
 drawBackgroundImage4(PauseBitmap);
 while(1)
 {
        oldButtons = buttons;
  buttons = *(volatile unsigned int *)0x04000130;
  if((!(~(oldButtons)&((1<<3))) && (~buttons & ((1<<3))))) {
   state = GAME;
   unpauseSound();
   waitForVblank();
   fillScreen4(0);
   return;
  }
  if((!(~(oldButtons)&((1<<2))) && (~buttons & ((1<<2))))) {
   initialize();
   state = GAME;
   unpauseSound();
   waitForVblank();
   fillScreen4(0);
   return;
  }
 }
}

